            <li>
                <HyperlinkButtonLogo Hyperlink="https://drive.google.com/file/d/1bm4Vwtk8o6wn_HDoRKwNAUYWCL4aa-cv/view?usp=drive_link" ButtonText="Resume" ButtonIconPath="SVGs/resume.svg"/>
            </li>
            <li>
                <HyperlinkButtonLogo Hyperlink="https://www.linkedin.com/in/wyatt-sanders/" ButtonText="LinkedIn" ButtonIconPath="SVGs/linkedin.svg" />
            </li>
            <li>
                <HyperlinkButtonLogo Hyperlink="https://discordapp.com/users/216400302553694209" ButtonText="Discord" ButtonIconPath="SVGs/discord.svg" />
            </li>
        </ul>
    </div>
</div>

                        

