@using BlazorApp2.Components.Pages.Resume.CircleImage

<h3>ResumePage</h3>

<div class="background">
    <div class="container">
        <CircleImage ImageSource="images/WyattHeadshot.jpg" AltDisplayText="Wyatt Sanders's Profile Photo" />
