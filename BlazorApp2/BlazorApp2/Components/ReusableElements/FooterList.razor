<section>
    @if (ListItems is null)
    {
        <p><em>loading ...</em></p>
    }
    else
    {
        <ul>
            @foreach (string item in ListItems)
            {
                <li>
                    @item
                </li>
            }
        </ul>
    }
</section>

@code {
    [Parameter]
    public required List<string> ListItems { get; set; }

}

    @foreach (string project in projects)
{
    @project
}
}