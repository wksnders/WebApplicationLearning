


<div class="header" id="myHeader">
    <div class="headerSection Left">
        @Left
    </div>
    <div class="headerSection Middle">
        @Middle
    </div>
    <div class ="headerSection Right">
        @Right
    </div>
</div>



@code {
    [Parameter]
    public RenderFragment? Left { get; set; }
    [Parameter]
    public RenderFragment? Middle { get; set; }
    [Parameter]
    public RenderFragment? Right { get; set; }


}
